@model IEnumerable<MvcMovie.Models.PRNews>  

    <!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Our Service</title>
    <link rel="stylesheet" href="css/bootstrap.css">
    <link rel="stylesheet" href="css/customs.css">
    <link rel="stylesheet" href="css/sidebar.css">
    <link rel="stylesheet" href="fonts/stylesheet.css" type="text/css" charset="utf-8" />
  
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css" />
    <link rel="stylesheet" href="css/splide.min.css">
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css" />
    <script src="https://kit.fontawesome.com/48956afbda.js" crossorigin="anonymous"></script>


</head>

<body>
  <div class="wrapper">
  <div class="jumbotron jumbotron-fluid banner-page " style="">
            <img class="" src="images/banner_news.png" alt="">
            <div class="container">
                @if(ViewBag.data=="th"){
                <h1 class="display-4">ข่าวสารและโปรโมชั่น</h1>
                <p class="lead">NEWS AND PROMOTION</p>
                }else if(ViewBag.data=="en"){
                         <h1 class="display-4">NEWS AND PROMOTION</h1>
                }else if(ViewBag.data=="jp"){
                

                     <h1 class="display-4">NEWS AND PROMOTION</h1>

                }
            </div>
        </div>

   
        <div class="modal fade" id="modaltool3" tabindex="-1" role="dialog" aria-labelledby="modaltool3Label"
            aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="modaltool3Label">คำนวณค่าคอมมิชชั่น</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <div role="form" class="form-container cal_detail mt-3">

                            <p>ซื้อ/ขายหุ้น (บาท)</p>
                            <!--<input id="txtBuy" type="text" onchange="calCommision();" oninput="this.value = this.value.replace(/[^0-9.]/g, ''); this.value = this.value.replace(/(\..*)\./g, '$1');this.onchange();" />-->
                            <input id="txtBuy" type="text" class="number"><br><br>

                            <label style="font-size:94.5%">ค่าคอมมิชชั่น *</label><br>
                            <label><span id="lblCom"></span></label><br>


                            <label style="font-size:94.5%">

                                ค่าธรรมเนียมตลาดฯ และ กำกับดูแล (0.007%)
                            </label><br>
                            <label><span id="lblFee"></span></label><br>

                            <label style="font-size:94.5%">
                                <label style="font-size:94.5%">ภาษีมูลค่าเพิ่ม (7%)</label>

                            </label><br>
                            <label><span id="lblTax"></span></label><br>

                            <label><span id="lblSum"></span></label><br>
                            <label><span style="color:red;font-size:94.5%">*ค่าคอมมิชชั่นที่ใช้คำนวณ
                                    เป็นค่าคอมมิชชั่นของบัญชีประเภท Cash Balance และ Credit Balance</span></label>
                            <div class="btn-footer">
                                <button type="button" class="btn btn-close" data-dismiss="modal">Close</button>

                            </div>

                        </div>
                    </div>

                </div>
            </div>
        </div>
        <div class="promotion mb-5">
            <div class="container">
                <div class="row">
                    <div class="col-xl-3 col-md-3 col-sm-3">
                        <section class="pull-right d-none d-md-block">
                            <ul class="sidebar-menu">

                                <li>
                                    <a  asp-controller="News" asp-action="Index">โปรโมชั่น</a>
                                </li>
                                <li>
                                    <a href="#">
                                        <span>News from SBITO </span> <i class="fa  fa-caret-right pull-right"></i>
                                    </a>
                                    <ul class="sidebar-submenu">
                                        <li><a asp-controller="Announcement" asp-action="Index">ประกาศและข่าวสาร


                                            </a></li>
                                        <li><a asp-controller="TurnoverandCaution" asp-action="Index"> Turnover and Caution report</a>
                                        </li>
                                        <li><a asp-controller="Xr" asp-action="Index"> XR / Warrant / Tender</a></li>
                                         <li><a asp-controller="Seminar" asp-action="Index"> สัมมนา</a></li>
                                    </ul>
                                </li>
                                <li>
                                   <a asp-controller="StockandOrganize" asp-action="Index">Stock exchange and
                                    organization </a>
                                </li>
                                <li  class="active">
                                     <a asp-controller="PRNew" asp-action="Index">PR NEWS</a>
                                </li>
                                <li>
                                    <a asp-controller="Career" asp-action="Index">
                                        <span>ร่วมงานกับ SBITO</span>
                                    </a>
                                </li>

                            </ul>
                            <div class="open_sidebar mt-5">
                                <img src="images/open_sidebar.png" alt="">
                            </div>

                        </section>

                    </div>
                    <div class="col-xl-9 col-md-9 col-sm-9">
                        <div class="head_promotion">
                            <h1>
                               PR News
                            </h1>
                        </div>
                 @foreach (var item in Model) {

               
                            <section class="topicnews">
                                <div class="cardsbito mt-5 mb-5">
                                    <div class="card mb-3">
                                        <div class="row no-gutters">
                                            <div class="col-md-2 col-12">
                                                <div class="calendar">
                                                    <h1>
                                                        17
                                                    </h1>
                                                    <h3>
                                                        มีนาคม
        
                                                    </h3>
                                                    <h3>
                                                        2021
                                                    </h3>
                                                </div>
                                            </div>
                                            <div class="col-md-10 col-12">
                                                <div class="card-body">
                                                    <div class="row">
                                                        <div class="col-lg-8 col-12">
                                                            <h3 class="card-title">@Html.DisplayFor(modelItem => item.title_th)</h3>
                                                            <p class="card-text">(views 7)</p>
        
                                                        </div>
                                                        <div class="col-lg-4 col-12">
                                                            <a asp-action="Details" asp-route-id="@item.id">
                                                                <button class="btn btn-more">รายละเอียด <i class="fas fa-long-arrow-alt-right"></i></button>
                                                            </a>
                                                        </div>
                                                    </div>
        
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </section>
  }
                            <nav aria-label="Page navigation example">
                            <ul class="pagination justify-content-start">
                              <li class="page-item active">
                                <a class="page-link" href="#" tabindex="-1" aria-disabled="true">Previous</a>
                              </li>
                              <li class="page-item"><a class="page-link" href="#">1</a></li>
                              <li class="page-item"><a class="page-link" href="#">2</a></li>
                              <li class="page-item"><a class="page-link" href="#">3</a></li>
                              <li class="page-item">
                                <a class="page-link" href="#">Next</a>
                              </li>
                            </ul>
                          </nav>
                        
                    

                            </div>
                        </div>
          
                    </div>

                </div>
            </div>
        </div>
      
    </div>
    <!-- wrapper -->
</body>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="js/calculatecommission.js"></script>
  

      
    </script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.min.js"
        integrity="sha384-w1Q4orYjBQndcko6MimVbzY0tgp4pWB4lZ7lr30WKz0vr/aWKhXdBNmNb5D92v7s" crossorigin="anonymous">
    </script>
  
    <script src="js/slick.js"></script>


    <script src="js/sidebar.js"></script>
    <script>
        $.sidebarMenu($('.sidebar-menu'))
    </script>
    <script>
        $.sidebarMenumb($('.sidebar-menu'))
    </script>
  